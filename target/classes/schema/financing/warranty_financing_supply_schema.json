{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "currency": {
              "type": "string"
            },
            "warrantyType": {
              "type": "string"
            },
            "warrantySubType": {
              "type": "string"
            },
            "warrantyAmount": {
              "type": "number"
            }
          },
          "required": [
            "currency",
            "warrantyType",
            "warrantySubType",
            "warrantyAmount"
          ]
        }
      ]
    },
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        },
        "first": {
          "type": "string"
        },
        "prev": {
          "type": "string"
        },
        "next": {
          "type": "string"
        },
        "last": {
          "type": "string"
        }
      },
      "required": [
        "self",
        "first",
        "prev",
        "next",
        "last"
      ]
    },
    "meta": {
      "type": "object",
      "properties": {
        "totalRecords": {
          "type": "integer"
        },
        "totalPages": {
          "type": "integer"
        },
        "requestDateTime": {
          "type": "string"
        }
      },
      "required": [
        "totalRecords",
        "totalPages",
        "requestDateTime"
      ]
    }
  },
  "required": [
    "data",
    "links",
    "meta"
  ]
}